{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "update": "npx npm-check -u",
    "update-types": "npx supabase login && supabase gen types typescript --project-id \"bnwwxhxmbmrgxwlizgeo\" > app/lib/supabase.d.ts",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -i ./tailwind.css -o ./app/root.css",
    "start": "remix-serve build",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@remix-run/node": "^1.7.6",
    "@remix-run/react": "^1.7.6",
    "@remix-run/vercel": "^1.7.6",
    "@supabase/supabase-js": "^2.1.1",
    "@vercel/node": "^2.7.0",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "numbro": "^2.3.6",
    "react": "^18.2.0",
    "react-circle-flags": "^0.0.18",
    "react-country-flag": "^3.0.2",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-spring": "npm:@react-spring/web@^9.5.5",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.7.6",
    "@remix-run/eslint-config": "^1.7.6",
    "@remix-run/serve": "^1.7.6",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.28.0",
    "npm-run-all": "^4.1.5",
    "supabase": "^1.17.0",
    "tailwindcss": "^3.2.4",
    "tiny-invariant": "^1.3.1",
    "typescript": "^4.8.4"
  }
}
